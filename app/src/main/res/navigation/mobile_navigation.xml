<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">
    <fragment
        android:id="@+id/home"
        android:name="com.example.cognibrowse.ui.home.HomeFragment"
        android:label="@string/home_page"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.cognibrowse.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/home2"
            app:destination="@id/home" />
    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.cognibrowse.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" >
        <action
            android:id="@+id/action_nav_gallery_to_home"
            app:destination="@id/home" />
    </fragment>

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.cognibrowse.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" >
        <action
            android:id="@+id/action_nav_slideshow_to_home"
            app:destination="@id/home" />
    </fragment>
</navigation>